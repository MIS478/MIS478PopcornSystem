<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.mis478.popcornapp.SalesMode"
    android:id="@+id/sup">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Sales Mode"
        android:id="@+id/SMlbl"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:id="@+id/BackBtn"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:onClick="Back" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Donate"
        android:id="@+id/DonateBtn"
        android:layout_alignTop="@+id/BackBtn"
        android:layout_toRightOf="@+id/BackBtn"
        android:layout_toEndOf="@+id/BackBtn"
        android:onClick="Donate" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total"
        android:id="@+id/TotalBtn"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/SMlbl"
        android:layout_alignEnd="@+id/SMlbl"
        android:onClick="Total" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/next"
        android:id="@+id/SalesNextButton"
        android:clickable="true"
        android:onClick="CustInfo"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pop1btn"
        android:src="@mipmap/pop1"
        android:layout_marginTop="54dp"
        android:onClick="one"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/textView13"
        android:layout_toStartOf="@+id/textView13" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pop2btn"
        android:src="@mipmap/pop2"
        android:layout_alignTop="@+id/pop1btn"
        android:layout_alignLeft="@+id/TotalBtn"
        android:layout_alignStart="@+id/TotalBtn"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="45dp"
        android:onClick="two" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Page 1"
        android:id="@+id/textView2"
        android:layout_below="@+id/SMlbl"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="36dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pop3btn"
        android:src="@mipmap/pop3"
        android:layout_marginTop="121dp"
        android:layout_below="@+id/pop1btn"
        android:layout_alignLeft="@+id/pop1btn"
        android:layout_alignStart="@+id/pop1btn"
        android:onClick="three" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pop4btn"
        android:src="@mipmap/pop4"
        android:layout_alignTop="@+id/pop3btn"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2"
        android:onClick="four" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Price: $5 "
        android:id="@+id/textView3"
        android:layout_below="@+id/pop1btn"
        android:layout_alignLeft="@+id/pop1btn"
        android:layout_alignStart="@+id/pop1btn" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Price: $5 "
        android:id="@+id/textView4"
        android:layout_below="@+id/pop2btn"
        android:layout_alignLeft="@+id/pop2btn"
        android:layout_alignStart="@+id/pop2btn" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Price: $5 "
        android:id="@+id/textView5"
        android:layout_below="@+id/pop3btn"
        android:layout_alignLeft="@+id/pop3btn"
        android:layout_alignStart="@+id/pop3btn" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Price: $5 "
        android:id="@+id/textView6"
        android:layout_below="@+id/pop4btn"
        android:layout_alignLeft="@+id/pop4btn"
        android:layout_alignStart="@+id/pop4btn" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Number Selected:"
        android:id="@+id/textView8"
        android:layout_below="@+id/textView3"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Number Selected:"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Number Selected:"
        android:id="@+id/textView11"
        android:layout_below="@+id/textView5"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignEnd="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Number Selected:"
        android:id="@+id/textView12"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView13"
        android:layout_below="@+id/textView8"
        android:layout_alignRight="@+id/DonateBtn"
        android:layout_alignEnd="@+id/DonateBtn" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/lbl1"
        android:text="0"
        android:layout_alignTop="@+id/textView13"
        android:layout_toRightOf="@+id/textView3"
        android:layout_toEndOf="@+id/textView3"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/lbl2"
        android:text="0"
        android:layout_alignTop="@+id/textView13"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/lbl3"
        android:text="0"
        android:layout_below="@+id/textView11"
        android:layout_alignLeft="@+id/lbl1"
        android:layout_alignStart="@+id/lbl1"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/lbl4"
        android:text="0"
        android:layout_alignTop="@+id/lbl3"
        android:layout_alignLeft="@+id/lbl2"
        android:layout_alignStart="@+id/lbl2"
        android:editable="true" />

</RelativeLayout>
