<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mis478.popcornapp"
    android:versionCode="7"
    android:versionName="0.1.3" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name=".Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CustomerInfo"

            android:label="@string/title_activity_customer_info"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".ScoutGoal"
            android:label="@string/title_activity_scout_goal"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".DeliveryMode"
            android:label="@string/title_activity_delivery_mode"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".PastSales"
            android:label="@string/title_activity_past_sales"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".DenLeaderMain"
            android:label="@string/title_activity_DenLeaderMain"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".DenSalesTracking"
            android:label="@string/title_activity_den_sales_tracking"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".ViewScouts"
            android:label="@string/title_activity_view_scouts"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".ScoutStatusPage"
            android:label="@string/title_activity_scout_status_page" >
        </activity>
        <activity
            android:name=".ScoutMainPage"
            android:label="@string/title_activity_scout_main_page" >
        </activity>
        <activity
            android:name=".SalesMode"
            android:label="@string/title_activity_sales_mode" >
        </activity>
        <activity
            android:name=".DeliveryCustomerInfo"
            android:label="@string/title_activity_delivery_customer_info"
            android:parentActivityName=".DeliveryMode" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mis478.popcornapp.DeliveryMode" />
        </activity>
        <activity
            android:name=".Donation"
            android:label="@string/title_activity_donation"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".OrderFinalization"
            android:label="@string/title_activity_order_finalization" >
        </activity>
    </application>

</manifest>
